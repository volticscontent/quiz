{
  "pixel_code": "{{ $json.tiktok_payload.pixel_code }}",
  "event": "CompletePayment",
  "event_id": "{{ $json.tiktok_payload.event_id }}",
  "timestamp": "{{ $json.tiktok_payload.timestamp }}",
  "context": {
    "user": {
      "email": "{{ $json.tiktok_payload.context.user.email }}",
      "phone_number": "{{ $json.tiktok_payload.context.user.phone }}",
      "external_id": "{{ $json.tiktok_payload.context.user.external_id }}"
    },
    "page": {
      "url": "{{ $json.tiktok_payload.context.page.url }}",
      "referrer": "{{ $json.tiktok_payload.context.page.referrer }}"
    },
    "ip": "127.0.0.1",
    "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
  },
  "properties": {
    "contents": [
      {
        "content_id": "{{ $json.tiktok_payload.properties.contents[0].content_id }}",
        "content_type": "product",
        "content_name": "{{ $json.tiktok_payload.properties.contents[0].content_name }}",
        "brand": "{{ $json.tiktok_payload.properties.contents[0].brand }}",
        "price": "{{ parseFloat($json.tiktok_payload.properties.contents[0].price) }}",
        "quantity": "{{ parseInt($json.tiktok_payload.properties.contents[0].quantity) }}"
      }
    ],
    "currency": "{{ $json.tiktok_payload.properties.currency }}",
    "value": "{{ parseFloat($json.tiktok_payload.properties.value) }}",
    "content_type": "product"
  }
} 